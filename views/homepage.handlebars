{{#each posts as |post|}}
<div class= "card">
    <div class="card-header">
        <a href="/singlePost/{{post.id}}"> {{post.title}}</a>
    </div>
    <div class="card-body">
            <p>{{post.contents}}</p>
    </div>
<div class="date">
    <P>{{post.createdAt}}</P>
</div>
</div>
    

{{/each}}

